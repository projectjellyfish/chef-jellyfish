# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
        . ~/.bashrc
fi

# User specific environment and startup programs

export PATH="/home/jellyfish/.rbenv/shims:/home/jellyfish/.rbenv/bin:$PATH"
eval "$(rbenv init -)"

export DATABASE_URL=postgres://<%= @dbuser %>:<%= @dbpasswd %>@localhost:5432/<%= @dbname %>
export RAILS_ENV=<%= @rails_env %>
